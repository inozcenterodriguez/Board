<UserControl x:Class="BoardDef.GamePlace"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="300" d:DesignWidth="400" Height="50" Width="50" Background="Transparent">
    
    <Grid x:Name="LayoutRoot" Background="Transparent">
        <Ellipse Fill="#FFFF6016"/>
        <Ellipse x:Name="powereli" Fill="Aquamarine" Visibility="Collapsed"/>
        <Ellipse x:Name="casualityeli" Fill="Yellow" Visibility="Collapsed"/>
        <Ellipse x:Name="pawn1" Fill="#FF162BFF" Margin="10" Visibility="Collapsed"/>
    </Grid>
</UserControl>
